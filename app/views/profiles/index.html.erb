<p id="notice"></p>

<p></p>
<div class="overlay"></div>

<div class="overlay" id ="bio-overlay"></div>
<div class="border-form-div expose center-absolute" id="tutorial-wrapper">
	 <h3>Welcome to Our Website Tutorial!</h3>
	 <p>To start off, please fill out your Anonymous Bio so that you can connect with your peers.</p>
	 <%= @typeform.embed_as(:modal, button_text: "Fill out my Anonymous Bio!").html_safe %>
</div>

<div class="container-fluid">
	<div class="row">
	  
	    <div id="default-tooltip" class="col-md-4 filters-container expose">
	        <legend class="filter-legend search-header" id="filter-tooltip">Filter by</legend>
	        <%= render '/profiles/search_partials/search_form' %>
	    </div>
	    
	    <div class="col-md-8 profile-results-container expose">
	        <legend style="margin-bottom:-5px;" class="search-header">
	        	Results
	        	<div class="users-found"><%= @num_profiles %> users found</div>
	        </legend>
	        <%= render '/profiles/search_partials/search_results' %>
	    </div>
	</div>
</div>

<%= render 'profiles/show_partials/profile_modal' %>
