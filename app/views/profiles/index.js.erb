if (!gon.doneTutFilter) {
	gon.doneTutFilter = true;
}

updateSearchContent('<%= j render("/profiles/search_partials/search_form") %>', '<%= j render("/profiles/search_partials/search_results") %>');
bindAgeSlider();
bindOnlineToggle();
bindPrefrenceSelectors();
$('div.users-found').html("<%= @num_profiles %> users found")

//initialize scrollbars
$('.custom-scroll').mCustomScrollbar({
    theme: "minimal-dark"
});

<% if current_user.peer_counselor %>
  <% if @sort_type == "recommend_count" %>
    disableSort('#recommend-sort');
  <% else %>
    bindRecommendSort();
  <% end %>
<% else %>
  <% if @sort_type == "age" %>
    disableSort('#age-sort');
    bindSort('#name-sort', '2');
  <% elsif @sort_type == "name" %>
    disableSort('#name-sort');
    bindSort('#age-sort', '1');
  <% else %>
    bindSort('#name-sort', '2');
    bindSort('#age-sort', '1');
  <% end %>
<% end %>